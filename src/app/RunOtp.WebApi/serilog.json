{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Postgresql.Alternative",
      "Serilog.Sinks.Telegram"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Telegram",
        "Args": {
          "botToken": "5142255196:AAFuIZLYaj-m9rce0Z8TiykPckxoLpDhEy8",
          "chatId": "736578147",
          "restrictedToMinimumLevel": "Error",
          "applicationName": "RunOtp",
          "includeStackTrace": true,
          "dateFormat": "yyyy-MM-dd HH:mm:sszzz"
        }
      },
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Log/mra_admin.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "Server=my.netcoreexamples.com;Port=5432;Database=run_otp;User Id=admin;Password=Mramra1234!@;",
          "tableName": "log",
          "schemaName": "data",
          "loggerColumnOptions": {
            "id": "IdAutoIncrement",
            "time_stamp": "Timestamp",
            "log_event": "LogEvent",
            "level": "LevelAsText",
            "properties": "Properties",
            "message": "Message",
            "message_template": "RenderedMessage",
            "exception": "Exception"
          },
          "restrictedToMinimumLevel": "Error",
          "period": "0.00:00:30",
          "batchSizeLimit": 50
        }
      }
    ]
  },
  "AllowedHosts": "*"
}